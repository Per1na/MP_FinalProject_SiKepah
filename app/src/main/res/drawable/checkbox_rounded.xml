<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Untuk keadaan checkbox yang sudah dicentang -->
    <item android:drawable="@drawable/checkbox_checked_rounded" android:state_checked="true"/>

    <!-- Untuk keadaan checkbox yang belum dicentang -->
    <item android:drawable="@drawable/checkbox_unchecked_rounded" android:state_checked="false"/>
</selector>
